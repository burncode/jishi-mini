<template name="progress">
  <view class='progress'>
    <text class='progress-title b'>答题进度</text>
    <text class='progress-number'>{{ progress.current_no }}/{{ progress.question_count }}</text>
    <view class='timer'>
    <text>剩余  00：{{ progress.leading_zero }}{{ progress.seconds }}</text>
</view>
    <progress stroke-width='20' percent='{{ progress.percent }}' activeColor='#00479d' />
  </view>
</template>