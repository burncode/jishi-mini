<import src="/pages/ceping/common/template.wxml" />
<template is="progress" data="{{progress}}" />
<text>\n</text>
<view class='option'>
<view class='harf-left'>
<text class='block'>A</text>
<text class='block'>{{question_items[current_key].sub_questions[0].title}}</text>
</view>
<view class='harf-right'>
<text class='block'>B</text>
<text class='block'>{{question_items[current_key].sub_questions[1].title}}</text>
</view>

</view>
<text>\n</text>
<view>
<radio-group class="radio-group" bindchange="radioChange">
  <label class="radio" wx:for="{{items}}">
  <view>
      <radio value="{{item.name}}" checked="{{item.checked}}"/>{{item.value}}
  </view>

  </label>
</radio-group>
</view>

<text>\n</text>

<button bindtap='{{bindfunction}}'>{{button_name}}</button>