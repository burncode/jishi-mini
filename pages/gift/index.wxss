
page {
  background-color: #f7f7f7;
}

.user {
  height: 178rpx;
  text-align: left;
  overflow: hidden;
  background-color: #fff;
}

.user-left {
  margin-left: 40rpx;
  margin-bottom: 34rpx;
}

.user image {
  display: block;
  width: 130rpx;
  height: 130rpx;
  float: left;
}

.user-info {
  margin-left: 30rpx;
  width: 490rpx;
  float: left;
  margin-top: -56rpx;
}

.user-info .user-name {
  margin-top: 40rpx;
  font-weight: bold;
  font-size: 32rpx;
  margin-bottom: 15rpx;
}

.user-info .ads-msg {
  color: #eb7675;
  font-size: 24rpx;
  margin-bottom: 40rpx;
}

.coupon {
  margin-top: 23rpx;
  width: 100%;
  height: 103rpx;
  background-color: #fff;
  overflow: hidden;
}

.coupon-left {
  margin-left: 40rpx;
}

.coupon .coupon-icon {
  display: block;
  width: 35rpx;
  height: 30rpx;
  float: left;
  margin-top: 35rpx;
}

.my-coupon {
  display: block;
  float: left;
  margin-left: 20rpx;
  margin-top: 30rpx;
  font-size: 28rpx;
}

.coupon-right {
  float: right;
}

.right-content {
  display: block;
  float: right;
  margin-top: 35rpx;
  font-size: 24rpx;
  color: #888;
  margin-right: 30rpx;
}

.right-icon {
  display: block;
  width: 12rpx;
  height: 21rpx;
  float: right;
  margin-right: 40rpx;
  margin-top: 40rpx;
}

.list-tab {
  height: 83rpx;
  background-color: #fff;
}

.tab {
  display: block;
  float: left;
  width: 50%;
  height: 83rpx;
  text-align: center;
  border-bottom: solid #f7f7f7 4rpx;
  font-size: 28rpx;
  line-height: 83rpx;
  color: #c3c3c3;
}

.tab.active {
  border-bottom: solid #306aaf 4rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #306aaf;
}

.order-list {
  padding-top: 23rpx;
  background-color: #fff;
  min-height: 516rpx;
}

.order-item {
  overflow: hidden;
}

.order-img {
  margin-left: 40rpx;
  margin-right: 40rpx;
  float: left;
}

.order-img image {
  display: block;
  width: 200rpx;
  height: 200rpx;
}

.order-content {
  float: left;
  width: 430rpx;
  margin-top: 35rpx;
}

.order-content .h1 {
  font-size: 30rpx;
  font-weight: bold;
}

.price {
  margin-top: 24rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #e64340;
}

.price-msg {
  margin-top: 24rpx;
  margin-left: 15rpx;
  font-size: 22rpx;
  color: #e1e1e1;
}

.del-price {
  margin-top: 24rpx;
  float: left;
  font-size: 24rpx;
  color: #888;
  text-decoration: line-through;
  margin-right: 40rpx;
}

.number {
  margin-top: 24rpx;
  font-size: 24rpx;
  color: #888;
  float: right;
}

.p {
  margin-left: 40rpx;
  font-size: 24rpx;
  color: #888;
  margin-bottom: 18rpx;
}

.order-date {
  margin-top: 30rpx;
}

.order-no {
  float: left;
}

.copy-no {
  float: right;
  font-size: 24rpx;
  color: #00479d;
  margin-right: 40rpx;
}

.order-buttons {
  clear: both;
  overflow: hidden;
  width: 100%;
  margin-bottom: 40rpx;
  border-bottom: solid #e5e5e5 4rpx;
}

.order-buttons text {
  display: block;
  float: right;
  width: 150rpx;
  height: 68rpx;
  margin-top: 30rpx;
  border: solid #00479d 2rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
  line-height: 68rpx;
  text-align: center;
  color: #00479d;
}

.order-buttons text.active {
  background-color: #00479d;
  margin-right: 40rpx;
  color: #fff;
}
.order-buttons view {
  float: right;
  width: auto;
  height: 68rpx;
  margin-top: 30rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
  line-height: 68rpx;
  text-align: center;
  color: #00479d;
}
button.present {
  float: right;
  width: 150rpx;
  height: 72rpx;
  margin-top: 30rpx;
  border: solid #00479d 2rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
  line-height: 72rpx;
  text-align: center;
  color: #00479d;
}
